<application>
  <component name="LanguageInjectionConfiguration">
    <option name="INSTRUMENTATION" value="ASSERT" />
    <option name="LANGUAGE_ANNOTATION" value="org.intellij.lang.annotations.Language" />
    <option name="PATTERN_ANNOTATION" value="org.intellij.lang.annotations.Pattern" />
    <option name="SUBST_ANNOTATION" value="org.intellij.lang.annotations.Subst" />
    <option name="RESOLVE_REFERENCES" value="true" />
    <injection language="GenericSQL" injector-id="xml">
      <display-name>input/@placeholder</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("placeholder")).withParent(xmlTag().withLocalName(string().equalTo("input")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="JavaScript 1.8" injector-id="xml">
      <display-name>input/@placeholder</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("placeholder")).withParent(xmlTag().withLocalName(string().equalTo("input")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="JavaScript 1.8" injector-id="xml">
      <display-name>label</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("label")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>